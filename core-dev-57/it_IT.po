#: concrete/config/install/base/permissions.xml:29
msgctxt "PermissionKeyName"
msgid "View"
msgstr "Visualizza"

#: concrete/config/install/base/permissions.xml:29
msgctxt "PermissionKeyDescription"
msgid "Can see a page exists and read its content."
msgstr "Può vedere pagine esistenti e leggerne il contenuto."

#: concrete/config/install/base/permissions.xml:30
msgctxt "PermissionKeyName"
msgid "View Versions"
msgstr "Vedi versioni"

#: concrete/config/install/base/permissions.xml:30
msgctxt "PermissionKeyDescription"
msgid "Can view the page versions dialog and read past versions of a page."
msgstr "Può vedere la finestra di dialogo delle versioni e leggere le versioni precedenti di una pagina."

#: concrete/config/install/base/permissions.xml:31
msgctxt "PermissionKeyName"
msgid "View Page in Sitemap"
msgstr "Vedi pagina nella mappa del sito"

#: concrete/config/install/base/permissions.xml:31
msgctxt "PermissionKeyDescription"
msgid ""
"Controls whether a user can see a page in the sitemap or intelligent search."
msgstr "Controlla se un utente può vedere una pagina nella mappa del sito o nella ricerca intelligente."

#: concrete/config/install/base/permissions.xml:32
msgctxt "PermissionKeyName"
msgid "Preview Page As User"
msgstr "Anteprima immagine come utente"

#: concrete/config/install/base/permissions.xml:32
msgctxt "PermissionKeyDescription"
msgid ""
"Ability to see what this page will look like at a specific time in the "
"future as a specific user."
msgstr "Possibilità di vedere come uno specifico utente vedrà questa pagina in un momento specifico nel futuro."

#: concrete/config/install/base/permissions.xml:33
msgctxt "PermissionKeyName"
msgid "Edit Properties"
msgstr "Modifica proprietà"

#: concrete/config/install/base/permissions.xml:33
msgctxt "PermissionKeyDescription"
msgid "Ability to change anything in the Page Properties menu."
msgstr "Possibilità di modificare qualsiasi cosa nel menu Proprietà pagina."

#: concrete/config/install/base/permissions.xml:34
msgctxt "PermissionKeyName"
msgid "Edit Contents"
msgstr "Modifica contenuti"

#: concrete/config/install/base/permissions.xml:34
msgctxt "PermissionKeyDescription"
msgid ""
"Ability to make edits to at least some of the content in the page. You can "
"lock down different block areas and specific blocks by clicking Permissions "
"on them as well. "
msgstr "Possibilità di apportare modifiche per almeno alcuni dei contenuti della pagina. È inoltre possibile bloccare varie aree blocchi e specifici blocchi facendo clic sulle relative autorizzazioni."

#: concrete/config/install/base/permissions.xml:35
msgctxt "PermissionKeyName"
msgid "Edit Speed Settings"
msgstr "Modifica impostazioni velocità"

#: concrete/config/install/base/permissions.xml:35
msgctxt "PermissionKeyDescription"
msgid "Ability to change caching settings."
msgstr "Possibilità di modificare le impostazioni della cache."

#: concrete/config/install/base/permissions.xml:36
msgctxt "PermissionKeyName"
msgid "Change Theme"
msgstr "Cambia tema"

#: concrete/config/install/base/permissions.xml:36
msgctxt "PermissionKeyDescription"
msgid "Ability to change just the theme for this page."
msgstr "Possibilità di cambiare solo il tema di questa pagina."

#: concrete/config/install/base/permissions.xml:37
msgctxt "PermissionKeyName"
msgid "Change Page Template"
msgstr "Cambia modello di pagina"

#: concrete/config/install/base/permissions.xml:37
msgctxt "PermissionKeyDescription"
msgid "Ability to change just the page template for this page."
msgstr "Possibilità di modificare solo il modello di pagina per questa pagina."

#: concrete/config/install/base/permissions.xml:38
#: concrete/config/install/base/permissions.xml:106
msgctxt "PermissionKeyName"
msgid "Edit Page Type"
msgstr "Modifica tipo di pagina"

#: concrete/config/install/base/permissions.xml:38
msgctxt "PermissionKeyDescription"
msgid "Change the type of an existing page."
msgstr "Cambia il tipo di una pagina esistente."

#: concrete/config/install/base/permissions.xml:39
#: concrete/config/install/base/permissions.xml:78
msgctxt "PermissionKeyName"
msgid "Edit Permissions"
msgstr "Modifica autorizzazioni"

#: concrete/config/install/base/permissions.xml:39
msgctxt "PermissionKeyDescription"
msgid ""
"Ability to change permissions for this page. Warning: by granting this a "
"user could give themselves more access."
msgstr "Possibilità di modificare le autorizzazioni per questa pagina. Attenzione: con la concessione di questa autorizzazione un utente potrebbe darsi ulteriori autorizzazioni."

#: concrete/config/install/base/permissions.xml:40
msgctxt "PermissionKeyName"
msgid "Delete"
msgstr "Elimina"

#: concrete/config/install/base/permissions.xml:40
msgctxt "PermissionKeyDescription"
msgid "Ability to move this page to the site's Trash."
msgstr "Possibilità di spostare questa pagina nel cestino del sito."

#: concrete/config/install/base/permissions.xml:41
msgctxt "PermissionKeyName"
msgid "Delete Versions"
msgstr "Elimina versioni"

#: concrete/config/install/base/permissions.xml:41
msgctxt "PermissionKeyDescription"
msgid "Ability to remove old versions of this page."
msgstr "Possibilità di rimuovere le versioni precedenti di questa pagina."

#: concrete/config/install/base/permissions.xml:42
msgctxt "PermissionKeyName"
msgid "Approve Changes"
msgstr "Approva modifiche"

#: concrete/config/install/base/permissions.xml:42
msgctxt "PermissionKeyDescription"
msgid "Can publish an unapproved version of the page."
msgstr "Può pubblicare e disapprovare le versioni della pagina."

#: concrete/config/install/base/permissions.xml:43
msgctxt "PermissionKeyName"
msgid "Add Sub-Page"
msgstr "Aggiungi sotto-pagina"

#: concrete/config/install/base/permissions.xml:43
msgctxt "PermissionKeyDescription"
msgid "Can add a page beneath the current page."
msgstr "Può aggiungere una pagina al di sotto della pagina attuale."

#: concrete/config/install/base/permissions.xml:44
msgctxt "PermissionKeyName"
msgid "Move or Copy Page"
msgstr "Sposta o copia pagina"

#: concrete/config/install/base/permissions.xml:44
msgctxt "PermissionKeyDescription"
msgid "Can move or copy this page to another location."
msgstr "Può spostare o copiare questa pagina in un’altra posizione."

#: concrete/config/install/base/permissions.xml:45
#: concrete/config/install/base/permissions.xml:68
#: concrete/config/install/base/permissions.xml:79
msgctxt "PermissionKeyName"
msgid "Schedule Guest Access"
msgstr "Pianifica accesso ospite"

#: concrete/config/install/base/permissions.xml:45
msgctxt "PermissionKeyDescription"
msgid "Can control scheduled guest access to this page."
msgstr "Può controllare l’accesso pianificato degli ospiti a questa pagina."

#: concrete/config/install/base/permissions.xml:46
msgctxt "PermissionKeyName"
msgid "Edit Multilingual Settings"
msgstr "Modifica le impostazioni multilingua"

#: concrete/config/install/base/permissions.xml:46
msgctxt "PermissionKeyDescription"
msgid ""
"Controls whether a user can see the multilingual settings menu, re-map a "
"page or set a page as ignored in multilingual settings."
msgstr "Controlla se un utente può vedere il menu delle impostazioni multilingua, se può rimappare una pagina o impostare una pagina come da ignorare nelle impostazioni multilingua."

#: concrete/config/install/base/permissions.xml:48
msgctxt "PermissionKeyName"
msgid "Add Block"
msgstr "Aggiungi blocco"

#: concrete/config/install/base/permissions.xml:48
msgctxt "PermissionKeyDescription"
msgid ""
"Can add a block to any area on the site. If someone is added here they can "
"add blocks to any area (unless that area has permissions that override these"
" global permissions.)"
msgstr "Può aggiungere un blocco a qualsiasi area del sito. Gli utenti presenti qui possono aggiungere blocchi a qualsiasi area (tranne per le aree che dispongono di autorizzazioni che sovrascrivono queste autorizzazioni globali)."

#: concrete/config/install/base/permissions.xml:53
msgctxt "PermissionKeyName"
msgid "Add Stack"
msgstr "Aggiungi pila"

#: concrete/config/install/base/permissions.xml:53
msgctxt "PermissionKeyDescription"
msgid ""
"Can add a stack or block from a stack to any area on the site. If someone is"
" added here they can add stacks to any area (unless that area has "
"permissions that override these global permissions.)"
msgstr "Può aggiungere una pila o un blocco da una pila a qualsiasi area del sito. Gli utenti presenti qui possono aggiungere pile a qualsiasi area (tranne per le aree che dispongono di autorizzazioni che sovrascrivono queste autorizzazioni globali)."

#: concrete/config/install/base/permissions.xml:60
msgctxt "PermissionKeyName"
msgid "View Area"
msgstr "Vedi area"

#: concrete/config/install/base/permissions.xml:60
msgctxt "PermissionKeyDescription"
msgid "Can view the area and its contents."
msgstr "Può visualizzare l’area e il suo contenuto."

#: concrete/config/install/base/permissions.xml:61
msgctxt "PermissionKeyName"
msgid "Edit Area Contents"
msgstr "Modifica contenuto area"

#: concrete/config/install/base/permissions.xml:61
msgctxt "PermissionKeyDescription"
msgid "Can edit blocks within this area."
msgstr "Può modificare i blocchi all’interno di questa area."

#: concrete/config/install/base/permissions.xml:62
msgctxt "PermissionKeyName"
msgid "Add Block to Area"
msgstr "Aggiungi blocco all’area"

#: concrete/config/install/base/permissions.xml:62
msgctxt "PermissionKeyDescription"
msgid ""
"Can add blocks to this area. This setting overrides the global Add Block "
"permission for this area."
msgstr "Può aggiungere blocchi a questa area. Questa impostazione sovrascrive le autorizzazioni globali di aggiunta blocchi, limitatamente a quest’area."

#: concrete/config/install/base/permissions.xml:63
msgctxt "PermissionKeyName"
msgid "Add Stack to Area"
msgstr "Aggiungi pila all’area"

#: concrete/config/install/base/permissions.xml:63
msgctxt "PermissionKeyDescription"
msgid ""
"Can add stacks to this area. This setting overrides the global Add Stack "
"permission for this area."
msgstr "Può aggiungere pile a questa area. Questa impostazione sovrascrive le autorizzazioni globali di aggiunta pile, limitatamente a quest’area."

#: concrete/config/install/base/permissions.xml:64
msgctxt "PermissionKeyName"
msgid "Add Layouts to Area"
msgstr "Aggiungi layout all’area"

#: concrete/config/install/base/permissions.xml:64
msgctxt "PermissionKeyDescription"
msgid ""
"Controls whether users get the ability to add layouts to a particular area."
msgstr "Controlla se gli utenti possono aggiungere layout a una specifica area."

#: concrete/config/install/base/permissions.xml:65
msgctxt "PermissionKeyName"
msgid "Edit Area Design"
msgstr "Modifica design area"

#: concrete/config/install/base/permissions.xml:65
msgctxt "PermissionKeyDescription"
msgid ""
"Controls whether users see design controls and can modify an area's custom "
"CSS."
msgstr "Controlla se gli utenti possono controllare e modificare lo stile personalizzato di un’area."

#: concrete/config/install/base/permissions.xml:66
msgctxt "PermissionKeyName"
msgid "Edit Area Permissions"
msgstr "Modifica autorizzazioni area"

#: concrete/config/install/base/permissions.xml:66
msgctxt "PermissionKeyDescription"
msgid ""
"Controls whether users can access the permissions on an area. Custom area "
"permissions could override those of the page."
msgstr "Controlla se gli utenti possono accedere alle autorizzazioni di un’area. Le autorizzazioni personalizzate dell’area sovrascrivono quelle della pagina."

#: concrete/config/install/base/permissions.xml:67
msgctxt "PermissionKeyName"
msgid "Delete Area Contents"
msgstr "Elimina contenuto area"

#: concrete/config/install/base/permissions.xml:67
msgctxt "PermissionKeyDescription"
msgid "Controls whether users can delete blocks from this area."
msgstr "Controlla se gli utenti possono eliminare i blocchi da quest’area."

#: concrete/config/install/base/permissions.xml:68
msgctxt "PermissionKeyDescription"
msgid ""
"Controls whether users can schedule guest access permissions on blocks in "
"this area. Guest Access is a shortcut for granting permissions just to the "
"Guest Group."
msgstr "Controlla se gli utenti possono pianificare autorizzazioni d’accesso ospite ai blocchi di quest’area. L’accesso come ospite è un collegamento per permettere autorizzazioni solo al gruppo ospiti."

#: concrete/config/install/base/permissions.xml:71
msgctxt "PermissionKeyName"
msgid "View Block"
msgstr "Vedi blocco"

#: concrete/config/install/base/permissions.xml:71
msgctxt "PermissionKeyDescription"
msgid "Controls whether users can view this block in the page."
msgstr "Controlla se gli utenti possono visualizzare questo blocco nella pagina."

#: concrete/config/install/base/permissions.xml:72
msgctxt "PermissionKeyName"
msgid "Edit Block"
msgstr "Modifica blocco"

#: concrete/config/install/base/permissions.xml:72
msgctxt "PermissionKeyDescription"
msgid ""
"Controls whether users can edit this block. This overrides any area or page "
"permissions."
msgstr "Controlla se gli utenti possono modificare questo blocco. Questa impostazione sovrascrive qualsiasi autorizzazione a livello di area o di pagina."

#: concrete/config/install/base/permissions.xml:73
msgctxt "PermissionKeyName"
msgid "Change Custom Template"
msgstr "Cambia modello personalizzato"

#: concrete/config/install/base/permissions.xml:73
msgctxt "PermissionKeyDescription"
msgid ""
"Controls whether users can change the custom template on this block. This "
"overrides any area or page permissions."
msgstr "Controlla se gli utenti possono cambiare il modello personalizzato di questo blocco. Questa impostazione sovrascrive qualsiasi autorizzazione a livello di area o di pagina."

#: concrete/config/install/base/permissions.xml:74
msgctxt "PermissionKeyName"
msgid "Edit Cache Settings"
msgstr "Modifica impostazioni della cache"

#: concrete/config/install/base/permissions.xml:74
msgctxt "PermissionKeyDescription"
msgid ""
"Controls whether users can change the block cache settings for this block "
"instance."
msgstr "Controlla se gli utenti possono modificare le impostazioni della cache per questa istanza del blocco."

#: concrete/config/install/base/permissions.xml:75
msgctxt "PermissionKeyName"
msgid "Edit Name"
msgstr "Modifica nome"

#: concrete/config/install/base/permissions.xml:75
msgctxt "PermissionKeyDescription"
msgid "Controls whether users can change the block's name (rarely used.)"
msgstr "Controlla se gli utenti possono modificare il nome del blocco (usato raramente)."

#: concrete/config/install/base/permissions.xml:76
msgctxt "PermissionKeyName"
msgid "Delete Block"
msgstr "Elimina blocco"

#: concrete/config/install/base/permissions.xml:76
msgctxt "PermissionKeyDescription"
msgid ""
"Controls whether users can delete this block. This overrides any area or "
"page permissions."
msgstr "Controlla se gli utenti possono eliminare questo blocco. Questa impostazione sovrascrive qualsiasi autorizzazione a livello di area o di pagina."

#: concrete/config/install/base/permissions.xml:77
msgctxt "PermissionKeyName"
msgid "Edit Design"
msgstr "Modifica design"

#: concrete/config/install/base/permissions.xml:77
msgctxt "PermissionKeyDescription"
msgid ""
"Controls whether users can set custom design properties or CSS on this "
"block."
msgstr "Controlla se gli utenti possono impostare proprietà personalizzate del design o stili CSS per questo blocco."

#: concrete/config/install/base/permissions.xml:78
msgctxt "PermissionKeyDescription"
msgid ""
"Controls whether users can change permissions on this block, potentially "
"granting themselves or others greater access."
msgstr "Controlla se gli utenti possono cambiare le autorizzazioni relative a questo blocco, assegnando potenzialmente a se stessi o ad altri ulteriori autorizzazioni."

#: concrete/config/install/base/permissions.xml:79
