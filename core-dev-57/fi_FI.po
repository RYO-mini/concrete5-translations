msgid "Can copy file."
msgstr "Voi kopioida tiedoston."

#: concrete/config/install/base/permissions.xml:95
msgctxt "PermissionKeyDescription"
msgid "Can edit access to file."
msgstr "Voi muokata pääsyoikeutta tiedostoon."

#: concrete/config/install/base/permissions.xml:96
msgctxt "PermissionKeyDescription"
msgid "Can delete file."
msgstr "Voi poistaa tiedoston."

#: concrete/config/install/base/permissions.xml:99
msgctxt "PermissionKeyName"
msgid "Approve or Deny"
msgstr "Hyväksy tai estä"

#: concrete/config/install/base/permissions.xml:99
msgctxt "PermissionKeyDescription"
msgid "Grant ability to approve workflow."
msgstr "Myönnä oikeus hyväksyä työnkulku."

#: concrete/config/install/base/permissions.xml:100
