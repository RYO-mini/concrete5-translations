msgid "Notify on Approve"
msgstr "Ilmoita hyväksymisellä"

#: concrete/config/install/base/permissions.xml:101
msgctxt "PermissionKeyDescription"
msgid "Notify approvers that a change has been approved."
msgstr "Ilmoita hyväksyjille, että muutos on hyväksytty."

#: concrete/config/install/base/permissions.xml:102
msgctxt "PermissionKeyName"
msgid "Notify on Deny"
msgstr "Ilmoita estämisellä"

#: concrete/config/install/base/permissions.xml:102
msgctxt "PermissionKeyDescription"
msgid "Notify approvers that a change has been denied."
msgstr "Ilmoita hyväksyjille, että muutos on hylätty."

