msgid "Flag Message"
msgstr "Merkitse viesti"

#: concrete/config/install/base/permissions.xml:124
msgctxt "PermissionKeyName"
msgid "Approve Message"
msgstr "Hyväksy viesti"

#: concrete/config/install/base/permissions.xml:127
msgctxt "PermissionKeyName"
msgid "Edit User Details"
msgstr "Muokata käyttäjän tietoja"

#: concrete/config/install/base/permissions.xml:133
msgctxt "PermissionKeyName"
