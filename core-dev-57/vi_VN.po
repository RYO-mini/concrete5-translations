msgstr "Có thể chỉnh sửa truy cập các tệp tin."

#: concrete/config/install/base/permissions.xml:96
msgctxt "PermissionKeyDescription"
msgid "Can delete file."
msgstr "Có thể xóa tệp tin."

#: concrete/config/install/base/permissions.xml:99
msgctxt "PermissionKeyName"
